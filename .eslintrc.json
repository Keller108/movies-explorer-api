{
    "extends": [
        "airbnb-base"
    ],
    "env": {
        "node": true,
        "es6": false,
        "browser": true
      },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "no-underscore-dangle": ["error", { "allow": ["_id", "_doc"] }]
    }
}